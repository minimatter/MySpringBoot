<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="end.dao.OrderDao">



    <select id="findOderByUid" resultType="end.bean.Orders">
        SELECT * FROM orders where uid=#{id,jdbcType=INTEGER}
    </select>

    <insert id="insertOrder" useGeneratedKeys="true" keyProperty="id" parameterType="end.bean.Orders">

        INSERT INTO orders (uid,total,addr,tel)
            VALUES (#{uid,jdbcType=INTEGER}, #{total,jdbcType=FLOAT},#{addr,jdbcType=VARCHAR},#{tel,jdbcType=VARCHAR} )
    </insert>



</mapper>
